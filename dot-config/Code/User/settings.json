{
    "[java]": {
        "editor.detectIndentation": false,
        "editor.foldingImportsByDefault": true,
        "editor.insertSpaces": false
    },
    "debug.autoExpandLazyVariables": "on",
    "debug.inlineValues": "off",
    "debug.javascript.autoAttachFilter": "disabled",
    "debug.onTaskErrors": "abort",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "editor.fontFamily": "Menlo, 'Symbols Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
    "editor.inlineSuggest.enabled": true,
    "editor.occurrencesHighlightDelay": 100,
    "editor.rulers": [
        120,
        150
    ],
    "editor.smoothScrolling": true,
    "editor.stickyScroll.defaultModel": "foldingProviderModel",
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.maxLineCount": 20,
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmPasteNative": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*.plantuml": "${basename}.svg",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb,node_modules"
    },
    "explorer.incrementalNaming": "smart",
    "extensions.ignoreRecommendations": true,
    "files.insertFinalNewline": true,
    "files.readonlyFromPermissions": true,
    "files.readonlyInclude": {
        "**/LICENSE": true,
        "**/node_moedules/**/*": true
    },
    "files.simpleDialog.enable": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.mergeEditor": true,
    "git.replaceTagsWhenPull": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.enable": {
        "*": true,
        "markdown": true,
        "plaintext": true,
        "scminput": false
    },
    "gitlens.autolinks": [
        {
            "alphanumeric": false,
            "ignoreCase": true,
            "prefix": "LC-",
            "url": "https://logicommerce.atlassian.net/browse/LC-<num>"
        }
    ],
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.defaultGravatarsStyle": "retro",
    "gitlens.launchpad.indicator.enabled": false,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "java.completion.matchCase": "off",
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "markdown.preview.scrollEditorWithPreview": false,
    "markdown.preview.scrollPreviewWithEditor": false,
    "security.workspace.trust.untrustedFiles": "open",
    "sonarlint.connectedMode.connections.sonarqube": [
        {
            "connectionId": "sonar-logicommerce",
            "serverUrl": "https://sonar-cicd.ci.logicommerce.systems"
        }
    ],
    "sonarlint.disableTelemetry": true,
    "sonarlint.pathToNodeExecutable": "~/.local/share/fnm/aliases/default/bin/node",
    "sonarlint.rules": {
        "java:S106": {
            "level": "off"
        },
        "java:S1123": {
            "level": "off"
        },
        "java:S1133": {
            "level": "off"
        },
        "java:S1874": {
            "level": "off"
        },
        "java:S2479": {
            "level": "off"
        },
        "java:S6813": {
            "level": "off"
        }
    },
    "supermaven.allowGitignore": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.tabs.location": "left",
    "testing.openTesting": "neverOpen",
    "update.showReleaseNotes": false,
    "window.confirmSaveUntitledWorkspace": false,
    "window.dialogStyle": "custom",
    "window.newWindowProfile": "Default",
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "workbench.editor.preventPinnedEditorClose": "keyboard",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.list.smoothScrolling": true,
    "workbench.startupEditor": "none"
}
